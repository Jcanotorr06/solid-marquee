{
  "version": "0.0.0",
  "name": "solid-marquee",
  "description": "A lightweight Solid component that utilizes the power of CSS animations to create silky smooth marquees. ",
  "license": "MIT",
  "author": "Jcanotorr06",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jcanotorr06/solid-marquee.git"
  },
  "homepage": "https://github.com/Jcanotorr06/solid-marquee#readme",
  "bugs": {
    "url": "https://github.com/Jcanotorr06/solid-marquee/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.4",
    "esbuild-plugin-solid": "^0.5.0",
    "jsdom": "^21.1.0",
    "prettier": "2.8.3",
    "solid-js": "^1.6.9",
    "tsup": "^6.5.0",
    "tsup-preset-solid": "^0.1.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.28.2"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@7.22.0"
}
